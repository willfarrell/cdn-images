{
  "name": "image-cdn",
  "version": "1.0.0",
  "description": "api to create responsive images",
  "main": "index.js",
  "scripts": {
    "start": "AWS_SDK_LOAD_CONFIG=1 AWS_PROFILE=datastream-development AWS_REGION=us-east-1 serverless offline start --noAuth --stage=cdn --environment=development --port=3000",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "mocha",
    "test-integration": "serverless invoke test",
    "deploy": "AWS_SDK_LOAD_CONFIG=1  ./node_modules/.bin/serverless deploy --SLS_DEBUG=* --verbose --force --stage=\"cdn\" --environment=${ENVIRONMENT} --aws-profile=${AWS_PROFILE_NAME}-${ENVIRONMENT}"
  },
  "author": "willfarrell",
  "license": "MIT",
  "dependencies": {
    "@middy/core": "1.0.0-alpha.41",
    "@middy/function-shield": "1.0.0-alpha.41",
    "@middy/http-cors": "1.0.0-alpha.41",
    "@middy/http-error-handler": "1.0.0-alpha.41",
    "@middy/http-event-normalizer": "1.0.0-alpha.41",
    "@middy/http-header-normalizer": "1.0.0-alpha.41",
    "@middy/http-security-header": "1.0.0-alpha.41",
    "@middy/validator": "1.0.0-alpha.41",
    "@middy/warmup": "1.0.0-alpha.41",
    "aws-sdk": "2.528.0",
    "sharp": "0.23.0",
    "svgo": "1.3.0",
    "ua-parser-js": "0.7.20"
  },
  "devDependencies": {
    "serverless": "1.52.0",
    "serverless-api-gateway-caching": "1.4.0",
    "serverless-apigw-binary": "0.4.4",
    "serverless-apigwy-binary": "1.0.0",
    "serverless-offline": "5.11.0",
    "serverless-plugin-tracing": "2.0.0",
    "serverless-prune-plugin": "1.4.1",
    "serverless-pseudo-parameters": "2.4.0"
  }
}
